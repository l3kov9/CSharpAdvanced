@model ICollection<MenuItemDetails>
<h2>Meals by Date</h2>
<div class="row">
    <div class="col-12">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>Order date</th>
                    <th>Meal name</th>
                    <th>Total portions</th>
                    <th>Clients who ordered this</th>
                </tr>
            </thead>
            <tbody>
                @Html.DisplayForModel()
            </tbody>
        </table>
    </div>
</div>